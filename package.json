{
    "name": "backend",
    "version": "0.1.0",
    "description": "Backend for the roleplay game",
    "main": "index.js",
    "browserslist": [
        "defaults",
        "maintained node versions"
    ],
    "scripts": {
        "start": "node dist/index.js",
        "clean": "rimraf dist",
        "lint": "eslint src/** --fix",
        "prebuild": "yarn clean && yarn lint",
        "build": "babel src -d dist --copy-files",
        "serve": "nodemon",
        "test": "jest --coverage"
    },
    "keywords": [
        "nodejs",
        "javascript",
        "rest-api"
    ],
    "author": "Alejandro Urdiales <alejandro.urdiales.93@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.8.6",
        "@babel/node": "^7.8.7",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-optional-chaining": "^7.8.3",
        "@babel/preset-env": "^7.8.6",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "jest": "^25.1.0",
        "nodemon": "^2.0.2",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.2"
    },
    "dependencies": {
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "swagger-ui-express": "^4.1.3"
    }
